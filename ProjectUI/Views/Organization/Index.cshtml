@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Organization/_Organization.cshtml";
}
@model IList<ProjectUI.Models.UserApp>

    <h3>User Table</h3>
         <table class="table table-dark table-hover">
               <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Name</th>
      <th scope="col">Surname</th>
      <th scope="col">UserName</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
             @foreach (var user in Model)
    {

  <tbody>
    <tr>
      <th scope="row">@user.Id</th>
      <td>@user.Name</td>
      <td>@user.Surname</td>
      <td>@user.UserName</td>
      <td>@user.Email</td>
    </tr>
  </tbody>
    }
</table>
    
   